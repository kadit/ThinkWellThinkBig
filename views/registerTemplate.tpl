<section id="register">
    <div class="container">
    	<div class="row">
    	    <div class="col-xs-12">
        	    <div class="form-wrap">
                <h1>Welcome to think well think big</h1>
                    <form role="form" action="javascript:;" method="post" id="login-form" autocomplete="off">
                        <div class="form-group">
                            <input type="email" name="userMailId" id="userMailId" class="form-control" placeholder="somebody@example.com">
                        </div>
                        <div class="form-group">
                            <input type="text" name="userName" id="userName" class="form-control" placeholder="Your fav name">
                        </div>

                        <div class="form-group">
                            <input type="password" name="userPassword" id="userPassword" class="form-control" placeholder="Password">
                        </div>
                        <div class="form-group">
                            <input type="text" name="userFirstName" id="userFirstName" class="form-control" placeholder="Your first name">
                        </div>
                        <div class="form-group">
                            <input type="text" name="userLastName" id="userLastName" class="form-control" placeholder="Your last name">
                        </div>

                         <div class="form-group">
                            <input type="text" name="userGuid" id="userGuid" class="form-control" placeholder="Invitation code received by mail">
                        </div>


                        <input type="submit" id="btn-register" class="btn btn-custom btn-lg btn-block" value="Register me">
                        <div id="registerInfo"></div>
                       
                    </form>
                    <hr>
        	    </div>
    		</div> 
    	</div> 
    </div> 
</section>